<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="key" format="string" />

    <declare-styleable name="TitleWidget">
        <attr name="title" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="AttributeWidget">
        <attr name="key" />
        <attr name="attr_value" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NestedAttributeWidget">
        <attr name="key" />
        <attr name="nested_attr_value" format="string|reference" />
        <attr name="value_type" format="enum">
            <enum name="string" value="0" />
            <enum name="color" value="1" />
            <enum name="integer" value="2" />
        </attr>
        <attr name="locked" format="boolean" />
    </declare-styleable>

</resources>